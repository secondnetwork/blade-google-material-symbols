{
    "name": "secondnetwork/blade-google-material-symbols",
    "description": "A package to easily make use of blade-google-material-symbols in your Laravel Blade views.",
    "keywords": ["Blade", "blade-google-material-symbols", "Laravel"],
    "homepage": "https://github.com/secondnetwork/blade-google-material-symbols",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Farah"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.8",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "secondnetwork\\MaterialSymbols\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "secondnetwork\\MaterialSymbols\\BladeMaterialSymbolsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
